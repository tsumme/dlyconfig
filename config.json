{
    "linkedin.com": {
        "company": {
         "selector": "[componentkey*='profile_experience'] + div div[componentkey^='entity-collection-item'] p:nth-of-type(2)",
         "transform": "innerText"
        },
        "container": {
            "selector": "[aria-label='Profile photo']"
        },
        "full_name": {
            "selector": "[data-view-name='profile-top-card-verified-badge'] p",
            "transform": "innerText"
        },
        "idUser": {
            "selector": "[data-member-id]",
            "transform": "data-member-id"
        },
        "location": {
            "selector": "div:has([data-view-name='profile-top-card-verified-badge']) div:nth-of-type(2) > p",
            "transform": "innerText"
        },
        "pic_url": {
            "selector": "img",
            "transform": "src"
        },
        "titles": {
            "selector": "div:has([data-view-name='profile-top-card-verified-badge']) p:nth-of-type(2)",
            "transform": "innerText"
        },
        "university": {
            "selector": "[componentkey*='profile_education'] + div p:nth-of-type(1)",
            "transform": "innerText"
        }
    },
    "mail.google.com": {
        "dom": {
            "selector": ".hx .gE"
        },
        "email": {
            "selector": ".gD",
            "transform": "email"
        },
        "nameprofile": {
            "selector": ".gD",
            "transform": "innerText"
        }
    },
    "version": "1.0.0"
}
