{
    "linkedin.com": {
        "company": {
         "selector": ".top-card-background-hero-image + div ul li:nth-of-type(1) button span div",
         "transform": "innerText"
        },
        "container": {
            "selector": "[aria-label='Profile photo']"
        },
        "full_name": {
            "selector": "[data-view-name='profile-top-card-verified-badge'] p",
            "transform": "innerText"
        },
        "idUser": {
            "selector": "[data-member-id]",
            "transform": "data-member-id"
        },
        "location": {
            "selector": "[data-view-name='profile-top-card-verified-badge']",
            "transform": "parentElement.parentElement.parentElement.parentElement.querySelector('div:nth-of-type(2) > p').innerText"
        },
        "pic_url": {
            "selector": "img",
            "transform": "src"
        },
        "titles": {
            "selector": ".text-body-medium.break-words",
            "transform": "innerText"
        },
        "university": {
            "selector": ".top-card-background-hero-image + div ul li:nth-of-type(2) button span div",
            "transform": "innerText"
        }
    },
    "mail.google.com": {
        "dom": {
            "selector": ".hx .gE"
        },
        "email": {
            "selector": ".gD",
            "transform": "email"
        },
        "nameprofile": {
            "selector": ".gD",
            "transform": "innerText"
        }
    },
    "version": "1.0.0"
}
